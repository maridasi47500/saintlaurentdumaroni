<div class="pager">
MYPAGER
<%if list.length <= 2 && params[:start].to_i <= 10%>
                        <%([1.to_s]+(0.to_s..(list.length.round(-1, half: :down) - 10).to_s).step(10).to_a.without("0")).each_with_index do |h,k|%>
			 <a <%if h == params[:start]%>class="sel"<%end%> href="<%=mysociallist_path(show: params[:show], l: params[:l], order: params[:order], photo: params[:photo], start: (k > 0 ? h : nil))%>"><%=(k+1)%></a>
                         <%end%>
                    <%elsif params[:start].to_i <= 10%>
                        <%([1.to_s]+(0.to_s..(list.length.round(-1, half: :down) - 10).to_s).step(10).to_a.without("0")).each_with_index do |h,k|%>
			 <a <%if h == params[:start]%>class="sel"<%end%> href="<%=mysociallist_path(show: params[:show], l: params[:l], order: params[:order], photo: params[:photo], start: (k > 0 ? h : nil))%>"><%=(k+1)%></a>
                         <%end%>
                    <%elsif params[:start].to_i < (list.length.round(-1, half: :down) - 10) %>
                        <%((params[:start].to_i - 10).to_s..(params[:start].to_i + 10).to_s).step(10).to_a.each_with_index do |h,k|%>
			 <a <%if h == params[:start]%>class="sel"<%end%> href="<%=mysociallist_path(show: params[:show], l: params[:l], order: params[:order], photo: params[:photo], start: (k > 0 ? h : nil))%>"><%=(h.to_i/10)%></a>
                         <%end%>

                    <%end%>
